types/movide\";\r\nimport { IMovieDbResult } from \"./types/movide-db-result\";\r\n\r\nexport class MainComponent {\r\n  getAndSetInitialData = async () => {\r\n    const data = await getData<IMovieDbResult>();\r\n    this.render(data);\r\n  };\r\n\r\n  render(data: IMovieDbResult): void {\r\n    const html = this.createMoviesTemplate(data.results);\r\n    this.setInnerHtml(document.body, html);\r\n  }\r\n\r\n  createMovieTemplate = (movie: IMovie): string => {\r\n    return `<div class=\"movie-card\">\r\n            <div><img src=\"https://image.tmdb.org/t/p/w200${movie.poster_path}\"/></div>\r\n            <div class=\"movie-card__title\">${movie.title}</div>\r\n            <div class=\"movie-card__footer\">\r\n                <span>Vote:${movie.vote_count}</span>\r\n                <span>Avarage:${movie.vote_avarage}</span>\r\n                <span>${movie.release_date}</span>\r\n            </div>\r\n    </div>`;\r\n  };\r\n\r\n  createMoviesTemplate = (movies: IMovie[]): string => {\r\n    return movies.map(this.createMovieTemplate).join(\"\");\r\n  };\r\n\r\n  setInnerHtml = (element: HTMLElement, innerHtml: string): void => {\r\n    element.innerHTML = innerHtml;\r\n  };\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"90e67ff8cb56b17732b89a7145907381","cacheData":{"env":{}}}